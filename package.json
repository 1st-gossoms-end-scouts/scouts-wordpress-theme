{
  "name": "scouts-wordpress-theme",
  "version": "0.0.0",
  "description": "A Wordpress template for scout organisations, compliant with the UK Scout Association brand guidelines. Gutenberg compatible. Using Bootstrap 5.x, Bootstrap Icons.x, and node-sass.",
  "main": "index.js",
  "scripts": {
    "scss": "node-sass --watch theme/scss -o theme/css",
    "scss-build": "node-sass theme/scss -o theme/css",
    "wp-env": "wp-env",
    "build": "wp-scripts build theme/jsx/*.js --output-path theme/js/\n",
    "dev": "wp-scripts start theme/jsx/*.js --output-path theme/js/\n",
    "build-blocks": "wp-scripts build theme/blocks/src/*/*.js --output-path=theme/blocks/build/",
    "dev-blocks": "wp-scripts start theme/blocks/src/*/*.js --output-path=theme/blocks/build/"
  },
  "watch": {
    "build": "js-src"
  },
  "author": "Harvey Allsop",
  "license": "GNU GPL V3",
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.3",
    "@wordpress/scripts": "^27.5.0",
    "@wordpress/block-editor": "^12.21.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "bootstrap": "^5.3.2",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "node-sass": "^9.0.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}
